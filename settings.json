// Place your settings in this file to overwrite the default settings
{
    "auto-rename-tag.activationOnLanguage": [
        "html",
        "xml",
        "php",
        "javascript"
    ],
    "breadcrumbs.enabled": true,
    "editor.fontFamily": "Consolas, 'Courier New', monospace",
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.minimap.enabled": true,
    "editor.tabSize": 2,
    "explorer.openEditors.visible": 0,
    "explorer.confirmDragAndDrop": false,
    "files.associations": {
        "**/**.cake": "csharp"
    },
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay: 3000,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/*.js.map": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewLines": true,
    "html.format.indentInnerHtml": true,
    "html.format.unformatted": "a, abbr, acronym, b, bdo, big, br, button, cite, code, dfn, em, i, img, input, kbd, label, map, object, q, samp,  small, span, strong, sub, sup, textarea, tt, var",
    "javascript.preferences.quoteStyle": "single",
    "javascript.updateImportsOnFileMove.enabled": true,
    "markdown.preview.scrollPreviewWithEditorSelection": false,
    "markdown.preview.scrollEditorWithPreview": false,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "tslint.enable": true,
    "tslint.run": "onSave",
    "tslint.autoFixOnSave": true,
    "typescript.check.tscVersion": false,
    "typescript.updateImportsOnFileMove.enabled": true,
    "workbench.iconTheme": "vs-seti",
    "window.zoomLevel": 1,
    "window.menuBarVisibility": "toggle",
    "workbench.colorTheme": "Default High Contrast",
    "zenMode.hideTabs": false,
}
